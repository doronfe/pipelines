pipeline{
    agent any
    stages{
        stage('Spellcheck'){
            steps{
                echo '[Main pipeline]: Check Spelling'
                build job: 'Spellcheck_Pipeline'
            }
        }
        stage('Linter'){
            steps{
                echo '[Main pipeline]: Check shell scripts'
            } 
        }
        stage('Build'){
            steps{
                echo 'Building the Project'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        }

}
